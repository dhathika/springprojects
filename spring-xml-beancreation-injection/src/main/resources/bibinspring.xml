<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

<bean id="customer" class="com.dhathika.beans.Customer" autowire="byName">
<property name="customerId" value="121212"></property>
<property name="customerName" value = "Sohel"></property>
<property name="customerContact" value="8970373795"></property>
</bean>

<bean id = "customerAddress" class="com.dhathika.beans.Address" autowire="byName">
<property name="doorNo" value="324"></property>
<property name="streetName" value = "bhagathstreet"></property>
<property name="city" value="Hyderabad"></property>
<property name="state" value="Telangana"></property>
<property name="pin" value = "515571"></property>
</bean>

<bean id = "landmark" class="com.dhathika.beans.Landmark" autowire="byName">
<property name="landmarkname" value="near water tank"></property>
<property name="landmarkdistance" value="100"></property>
</bean>
	
</beans>